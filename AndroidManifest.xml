<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.scorpion.covid">

    <uses-permission
        android:name="android.permission.GET_ACCOUNTS"
        android:maxSdkVersion="22" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/covid3"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CommunityActivity" />
        <activity android:name=".TracerActivity" />
        <activity android:name=".UserActivity" />
        <activity android:name=".TempActivity" />
        <activity android:name=".TravellerActivity" />
        <activity android:name=".ContactActivity" />
        <activity android:name=".CoronaActivity" />
        <activity android:name=".ParticularActivity" />
        <activity android:name=".VerifyActivity" />
        <activity android:name=".TravelActivity" />
        <activity android:name=".SymptomsActivity" />
        <activity android:name=".AboutActivity" />
        <activity android:name=".HelpActivity" />
        <activity android:name=".RateActivity" />
        <activity android:name=".HistoryActivity" />
        <activity android:name=".FeedbackActivity" />
        <activity android:name=".CovidActivity" />
        <activity android:name=".ForgotPasswordActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>